<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/set_Layout"
    android:background="@color/colorWhite"
    tools:context="com.androidy.azsecuer.activity.TelServiceActivity">

    <include
        layout="@layout/include_actionbar"
        android:id="@+id/tel_service_actionbar"/>

    <TextView
        style="@style/tel_title"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/tel_service_title" />

    <ListView
        android:background="@color/colorWhite"
        style="@style/tel_list_view"
        android:layout_marginTop="1dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:id="@+id/tel_service_listview" />

</LinearLayout>
